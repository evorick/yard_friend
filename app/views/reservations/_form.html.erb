<%= simple_form_for(@reservation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :listing %>
    <%= f.input :check_in %>
    <%= f.input :check_out %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
